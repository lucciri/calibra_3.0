;
var urlParams = GetURLParams(window.location.search);

// store the hash (DON'T put this code inside the $() function, it has to be executed 
// right away before the browser can start scrolling!
var targetHash = window.location.hash;
// delete hash so the page won't scroll to it
if (targetHash) {
    window.location.hash = "";
}

// Checks if the browser if IOS
Modernizr.addTest('isios', function () {
    return navigator.userAgent.match(/(iPad|iPhone|iPod)/g);
});

$(document).ready(function () {
    if (targetHash) {
        var target = $(targetHash);
        if ((target.length > 0) && !IsScrolledIntoView(target)) {
            setTimeout(function () {
                ScrollPageIntoView(target);
                window.location.hash = targetHash;
            }, 1000);
        }
    }

    $("a").filter(function () {
        // Filter out links without a hash and list-jump links on the reports page
        return (this.href.indexOf("episerver/cms") === -1 && this.hash !== "" && $(this).closest('.list-jump').length === 0 && this.pathname === window.location.pathname);
    }).on('click', function (event) {
        // Prevent default anchor click behavior
        event.preventDefault();

        // Store hash
        var hash = this.hash;
        var target = $(hash);
        if ((target.length > 0) && !IsScrolledIntoView(target)) {
            setTimeout(function () {
                ScrollPageIntoView(target);
                window.location.hash = hash;
            }, 1000);
        }
    });

    $('.copy-link').on('click', function (event) {
        event.preventDefault();
        CopyToClipboard(document.location.href, function () {
            $('.copy-link').tooltip('enable').tooltip('open');
        });
        return false;
    });

    $('a.copy-link').tooltip({
        disabled: true,
        content: "Link Copied",
        close: function (event, ui) { $(this).tooltip('disable'); },
        position: {
            my: "center bottom",
            at: "center top-16",
            collision: "none"
        }
    });
});

function FallbackCopyTextToClipboard(text, successCallback) {
    var textArea = document.createElement("textarea");
    textArea.value = text;

    // Avoid scrolling to bottom
    textArea.style.top = "0";
    textArea.style.left = "0";
    textArea.style.position = "fixed";

    document.body.appendChild(textArea);
    textArea.trigger("focus");
    textArea.select();

    try {
        result = document.execCommand('copy');
        if (result) {
            successCallback();
        }
    } catch (err) {}

    document.body.removeChild(textArea);
}
function CopyToClipboard(text, successCallback) {
    if (!navigator.clipboard) {
        return FallbackCopyTextToClipboard(text, successCallback);
    }

    result = navigator.clipboard.writeText(text).then(function () {
        successCallback();
    }, function (err) { });
}

// search box
function ClearDefault(control) {
    if ((control.value !== null) && control.value === control.attributes.getNamedItem("DefaultText").value) {
        control.value = "";
    }
}

function RestoreDefault(control) {
    if (control.value === "") {
        control.value = control.attributes.getNamedItem("DefaultText").value;
    }
    return false;
}

function SupportsHistoryApi() {
    return !!(window.history && history.pushState);
}

function ClearNewsSearch() {
    $("input#news-search-input").val('');
    $("form#news-list-form").trigger("submit");
}

function ClearBlogsSearch() {
    $("input#blogs-search-input").val('');
    $("form#blogs-list-form").trigger("submit");
}

function GetOptanonActiveGroups() {
    try {
        if (typeof OptanonActiveGroups !== 'undefined') {
            return OptanonActiveGroups.slice(1, -1).split(",");
        }
    } catch (err) { /*console.log("Couldn't Load Optanon Groups");*/ }

    return null;
}

function OptanonActiveGroupsContains(category) {
    var groups = GetOptanonActiveGroups();

    // for preprod envs that don't load GTM, return true
    if (groups == null && window.location.hostname != "www.svb.com")
        return true;

    if (groups !== null && (typeof category !== 'undefined') && category.length !== 0) {
        return ($.inArray(category.toUpperCase(), groups) >= 0);
    }

    return false;
}

function GetURLParams(url) {
    var urlParams = [];
    try {
        urlParams = (function (a) {
            if (a === "") return {};
            var b = {};
            for (var i = 0; i < a.length; ++i) {
                var p = a[i].split('=', 2);
                if (p.length > 1) {
                    if (p[1]) p[1] = decodeURIComponent(p[1].replace(/\+/g, " "));
                } else {
                    p[1] = "";
                }
                b[p[0]] = p[1];
            }
            return b;
        })((url.split('?'))[1].split('&'));
    }
    catch (err) { /*console.log("Couldn't parse params");*/ }

    return urlParams
}

function OpenSVBDialog(id, w, h) {
    if (typeof (id) !== "undefined") {
        var svbDialog = $("#svb-dialog");

        if (!(svbDialog.is(":data(uiDialog)"))) {
            if ((typeof (w) === "undefined") || isNaN(parseInt(w))) {
                w = 630;
            }

            if ((typeof (h) === "undefined") || isNaN(parseInt(h))) {
                h = "auto";
            }

            svbDialog.dialog({
                autoOpen: false,
                width: w,
                height: h,
                minWidth: 300,
                maxWidth: 900,
                modal: true,
                open: function (event, ui) {
                    // new for jQueryUI 1.10, they use a ui-front style that we need to override on the dialog
                    $(this).parent().css("z-index", "10002");

                    $.post("/handlers/GetContent/",
                        { "id": id },
                        function (data) {
                            if (data !== "") {
                                $("#svb-dialog-content").html(data);
                                EnableSpeedBumpLinks();
                            }
                        }
                    );

                },
                close: function (event, ui) {
                    //clear content in dialog
                    $("#svb-dialog-content").empty();
                    $(this).dialog("destroy");
                }
            });
        }

        svbDialog.dialog("open");
        $.ui.dialog.overlayInstances = 1;
    }
}

function IsScrolledIntoView(elem) {
    try {
        var docViewTop = $(window).scrollTop();
        var docViewBottom = docViewTop + $(window).height();
        var elemTop = $(elem).offset().top;
        var elemBottom = elemTop + $(elem).height();

        //Is more than half of the element visible
        return ((elemTop + ((elemBottom - elemTop) / 2)) >= docViewTop && ((elemTop + ((elemBottom - elemTop) / 2)) <= docViewBottom));
    }
    catch (err) {
        return true;
    }
}

function ScrollPageIntoView(obj) {
    var $n01 = $('.N01');
    var $svbn5b = $('.svbn5b');
    var headerheight = 0;

    if ($svbn5b.length) {
        headerheight = $svbn5b.height();
    } else if ($n01.length) {
        headerheight = $n01.height();
    }
   
    var pos = obj.offset().top - headerheight;
    $("html, body").animate({
        scrollTop: pos
    }, 1000);
}

function CheckTwitterLength(text) {
    var maxlength = 140;
    var object = document.getElementById(text.id)  //get your object
    if (object.value.length > maxlength) {
        object.trigger("focus"); //set focus to prevent jumping
        object.value = text.value.substring(0, maxlength); //truncate the value
        object.scrollTop = object.scrollHeight; //scroll to the end to prevent jumping
        return false;
    }
    return true;
}

var resize_timeout;
$(window).on('resize orientationchange', function () {
    clearTimeout(resize_timeout);
    resize_timeout = setTimeout(function () {
        $(window).trigger('resized');
    }, 200);
});

(function ($) {

    $.fn.FontAwesomeUnorderedList = function (iconCss) {
        this.each(function () {
            $ul = $(this).find("ul");
            if (!$ul.hasClass("fa-ul")) {
                $ul.addClass("fa-ul");
                $ul.children("li").each(function () {
                    $li = $(this);
                    if ($li.html().indexOf("fa-li") === -1) {
                        $li.prepend('<span class="fa-li"><span class="' + iconCss + '">&nbsp;</span>');
                    }
                });
            }
        });
    };

    // Equalize the height of the object, setting the min-height of each
    $.fn.EqualizeHeights = function (minHeight) {
        var maxHeight = 0;

        // clear min-heights before finding new heights
        $(this).css('min-height', '');

        this.each(function () {
            var $this = $(this);

            // Find the max height of all the "visible" elements
            if (($this.height() > maxHeight) && $this.is(":visible")) {
                maxHeight = $this.height();
            }
        });

        if ((typeof minHeight !== "undefined") && (maxHeight < minHeight)) {
            maxHeight = minHeight;
        }

        if (maxHeight > 0) {
            // Set the min-height of the elements
            this.css('min-height', maxHeight + 'px');
        }
    };

    $.fn.EqualizeOuterHeights = function (minHeight) {
        var maxHeight = 0;

        // clear min-heights before finding new heights
        $(this).css('min-height', '');

        this.each(function () {
            var $this = $(this);

            // Find the max height of all the "visible" elements
            if (($this.outerHeight() > maxHeight) && $this.is(":visible")) {
                maxHeight = $this.outerHeight();
            }
        });

        if ((typeof minHeight !== "undefined") && (maxHeight < minHeight)) {
            maxHeight = minHeight;
        }

        if (maxHeight > 0) {
            // Set the min-height of the elements
            this.css('min-height', maxHeight + 'px');
        }
    };

    /*!
    * FitText.js 1.2
    *
    * Copyright 2011, Dave Rupert http://fittextjs.com/
    * Released under the WTFPL license
    * http://sam.zoy.org/wtfpl/
    *
    * Date: Thu May 05 14:23:00 2011 -0600
    */
    $.fn.fitText = function (kompressor, options) {
        // Setup options
        var compressor = kompressor || 1,
            settings = $.extend({
                'minFontSize': Number.NEGATIVE_INFINITY,
                'maxFontSize': Number.POSITIVE_INFINITY
            }, options);

        return this.each(function () {
            // Store the object
            var $this = $(this);
            // Resizer() resizes items based on the object width divided by the compressor * 10
            var resizer = function () {
                $this.css('font-size', Math.max(Math.min($this.width() / (compressor * 10), parseFloat(settings.maxFontSize)), parseFloat(settings.minFontSize)));
            };
            // Call once to set.
            resizer();
            // Call on resize. Opera debounces their resize by default.
            $(window).on('resize.fittext orientationchange.fittext', resizer);
        });
    };
})(jQuery);

//Fix The problem is caused by an incompatability between knockout.js and jQuery. knockout has a built in dependency on jQuery's .clean() method when jQuery's jQuery/$ objects are detected on the page.
//If you load your own version of jQuery, you trigger this dependency.The.clean() method was deprecated and is no longer part of jQuery as of jQuery 1.9.Anyone using jQuery 1.9+ sees this error.
(function (jQuery, window, undefined) {
    // Since jQuery.clean is used internally on older versions, we only shim if it's missing
    if (!jQuery.clean) {
        jQuery.clean = function (elems, context, fragment, scripts) {
            // Set context per 1.8 logic
            context = context || document;
            context = !context.nodeType && context[0] || context;
            context = context.ownerDocument || context;

            migrateWarn("jQuery.clean() is deprecated");

            var i, elem, handleScript, jsTags,
                ret = [];

            // Complex logic lifted directly from jQuery 1.8
            if (fragment) {
                // Special handling of each script element
                handleScript = function (elem) {
                    // Check if we consider it executable
                    if (!elem.type || rscriptType.test(elem.type)) {
                        // Detach the script and store it in the scripts array (if provided) or the fragment
                        // Return truthy to indicate that it has been handled
                        return scripts ?
                            scripts.push(elem.parentNode ? elem.parentNode.removeChild(elem) : elem) :
                            fragment.appendChild(elem);
                    }
                };

                for (i = 0; (elem = ret[i]) !== null; i++) {
                    // Check if we're done after handling an executable script
                    if (!(jQuery.nodeName(elem, "script") && handleScript(elem))) {
                        // Append to fragment and handle embedded scripts
                        fragment.appendChild(elem);
                        if (typeof elem.getElementsByTagName !== "undefined") {
                            // handleScript alters the DOM, so use jQuery.merge to ensure snapshot iteration
                            jsTags = jQuery.grep(jQuery.merge([], elem.getElementsByTagName("script")), handleScript);

                            // Splice the scripts into ret after their former ancestor and advance our index beyond them
                            ret.splice.apply(ret, [i + 1, 0].concat(jsTags));
                            i += jsTags.length;
                        }
                    }
                }
            }

            return ret;
        };
    }
})(jQuery);

// jquery-safeform
// https://github.com/caxap/jquery-safeform/blob/master/jquery.safeform.js
(function ($) {
    var getTime = function () { return new Date().getTime(); };
    var SafeForm = function (element, options) {
        this.initialize(element, options);
    }

    SafeForm.prototype = {
        constructor: SafeForm,
        initialize: function (element, options) {
            this.$element = $(element);
            this.options = $.extend({}, $.fn.safeform.defaults, options);
            this.disabled = false;
            this.submittedAt = getTime();
            this.$element.submit($.proxy(this.prevenDoubleSubmit, this));
        },
        disable: function () {
            this.disabled = true;
        },
        complete: function () {
            this.disabled = false;
        },
        submit: function () {
            this.$element.trigger("submit");
        },
        isAutoRefreshed: function () {
            var o = this.options,
                now = getTime();
            return o.timeout && now - this.submittedAt > o.timeout;
        },
        prevenDoubleSubmit: function (event) {
            var o = this.options;
            if (this.disabled && !this.isAutoRefreshed()) return false;
            this.disable();
            this.submittedAt = getTime();
            if (typeof o.submit === "function") {
                return o.submit.call(this.$element, event);
            }
        }
    };

    $.fn.safeform = function (option) {
        return this.each(function () {
            var $this = $(this),
                data = $this.data('safeform'),
                options = typeof option === 'object' && option;
            if (!data) $this.data('safeform', (data = new SafeForm(this, options)));
            if (typeof option === 'string') data[option]();
        });
    };

    $.fn.safeform.Constructor = SafeForm;

    $.fn.safeform.defaults = {
        timeout: null,
        submit: null
    };

})(window.jQuery);

// Detects the visitor's location using geolocation API 
function DetectGeolocation() {
    // Prompt for location only if currently scope map is derived from IP address, no session user or cookie
    if (((GetCookie("GeoLocationLatitude") !== null) && (GetCookie("GeoLocationLongitude") !== null)) || !navigator.geolocation) {
        return;
    }

    navigator.geolocation.getCurrentPosition(DetectGeolocationSuccess, DetectGeolocationFailure);

    window.dataLayer = window.dataLayer || [];
    dataLayer.push({
        "Value": "Initiate",
        "event": "Geolocation"
    });
}

// Handles successful detection of visitor's location (i.e. user permitted detection)
function DetectGeolocationSuccess(position) {
    if (typeof (google) == "undefined") return;

    window.dataLayer = window.dataLayer || [];
    dataLayer.push({
        "Value": "Accept",
        "event": "Geolocation"
    });

    var geocoder = new google.maps.Geocoder();
    var latlng = { lat: parseFloat(position.coords.latitude), lng: parseFloat(position.coords.longitude) };
    geocoder.geocode({ 'location': latlng }, function (results, status) {
        if (status !== google.maps.GeocoderStatus.OK) {
            return;
        }

        window.dataLayer = window.dataLayer || [];
        dataLayer.push({
            "Value": "Reverse Geocode Success",
            "event": "Geolocation"
        });

        var address_components = results[0]['address_components'];

        if (OptanonActiveGroupsContains("C0003")) {
            // Gather location information
            for (var i = 0; i < address_components.length; i++) {
                switch (address_components[i]['types'][0]) {
                    case 'locality':
                        SetCookie("GeoLocationCity", address_components[i]['long_name'], 1);
                        break;
                    case 'administrative_area_level_1':
                        SetCookie("GeoLocationState", address_components[i]['short_name'], 1);
                        break;
                    case 'country':
                        SetCookie("GeoLocationCountry", address_components[i]['long_name'], 1);
                        break;
                }
            }

            SetCookie("GeoLocationLatitude", results[0]['geometry']['location'].lat(), 1);
            SetCookie("GeoLocationLongitude", results[0]['geometry']['location'].lng(), 1);
        }
        // refresh page using new location
        document.location.href = document.location.href;
    });
}

//Handles unsuccessful detection of visitor's location (i.e. user declined permission for detection)
function DetectGeolocationFailure() {
    window.dataLayer = window.dataLayer || [];
    dataLayer.push({
        "Value": "Deny",
        "event": "Geolocation"
    });
}

function GetGAClientID() {
    try {
        var tracker = ga.getAll()[0];
        return tracker.get('clientId');
    }
    catch (err) { /*console.log("Couldn't parse params");*/ }
    return "";
}

function SetCookie(name, value, days) {
    var expires = "";
    var domain = window.location.hostname;
    if (days) {
        var date = new Date();
        date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
        expires = "; expires=" + date.toGMTString();
    }
    document.cookie = name + "=" + value + expires + "; path=/; secure; domain=" + domain + ";";
}

function CookieExists(name) {
    var value = "; " + document.cookie;
    var parts = value.split("; " + name + "=");
    if (parts.length == 2) {
        return true;
    }
}

function GetCookie(name) {
    var nameEQ = name + "=";
    var ca = document.cookie.split(';');
    for (var i = 0; i < ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) === ' ') c = c.substring(1, c.length);
        if (c.indexOf(nameEQ) === 0) return c.substring(nameEQ.length, c.length);
    }
    return null;
}

function ClearCookies() {
    if (confirm("Are you sure you want to delete all your SVB.com cookies?")) {
        var cookies = document.cookie.split("; ");
        var date = new Date("July 4, 1776 00:00:00");
        var dateStr = date.toGMTString();
        for (var c = 0; c < cookies.length; c++) {
            document.cookie = cookies[c].split(";")[0].split("=")[0] + "=; expires=" + dateStr + "; path=/";
            var d = window.location.hostname.split(".");
            while (d.length > 0) {
                var cookieBase1 = name + '=; expires=' + dateStr + '; domain=' + d.join('.') + ' ;path=';
                var cookieBase2 = name + '=; expires=' + dateStr + '; domain=.' + d.join('.') + ' ;path=';
                var p = location.pathname.split('/');
                document.cookie = cookieBase1 + '/'; document.cookie = cookieBase2 + '/';
                while (p.length > 0) {
                    document.cookie = cookieBase1 + p.join('/'); document.cookie = cookieBase2 + p.join('/');
                    p.pop();
                };
                d.shift();
            }
        }
        alert("Your cookies have been deleted.");
    }
    window.location = "";
}

function CheckOverflow(el) {
    var curOverflow = el.style.overflow;
    if (!curOverflow || curOverflow === "visible")
        el.style.overflow = "hidden";
    // The + 1 with client width/height fixes a weird IE issue where the size shrinks by 1 pixel when you change the overflow to hidden
    var isOverflowing = (el.clientWidth + 1) < el.scrollWidth
        || (el.clientHeight + 1) < el.scrollHeight;

    el.style.overflow = curOverflow;

    return isOverflowing;
}

// Create Cookie, so we can track in GA if they're a client. We're assuming if they click on a link to to log into a banking system they're a client
function CreateClientCookie() {
    if (OptanonActiveGroupsContains("C0004")) {
        SetCookie("ecmClient", true, 365);
    }
}

