                e.preventDefault(); // Prevent the link from loading the page
            }).fancybox({
                width: 1600 / 2,
                height: 900 / 2,
                aspectRatio: true,
                padding: 10,
                afterShow: H01__video_pause,
                afterClose: H01__video_play,
                arrows: false,
                helpers: {
                    media: {}
                },
                youtube: {
                    rel: 0,
                    modestbranding: 1,
                    origin: location.protocol + '//' + location.hostname
                }
            });
        });
    };

