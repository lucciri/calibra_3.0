    // Fix to add missing ektron class that prevent double submit with forms
    $("form").find("input[name='EktFormId']").parent().addClass("ektron-ui-form");


    // Social Sharing and Sharing Modules Tracking in GA
    $(".ss-twitter, .s-twitter").on("click", function (e) {
        window.dataLayer = window.dataLayer || [];
        dataLayer.push({
            'event': 'socialInteraction',
            'socialNetwork': 'Twitter',
            'socialAction': 'Share',
            'socialTarget': document.location.toString().split("?")[0] // strip off any url variables
        });
    });

    $(".ss-facebook, .s-facebook").on("click", function (e) {
        window.dataLayer = window.dataLayer || [];
        dataLayer.push({
            'event': 'socialInteraction',
            'socialNetwork': 'Facebook',
            'socialAction': 'Share',
            'socialTarget': document.location.toString().split("?")[0] // strip off any url variables
        });
    });

    $(".ss-linkedin, .s-linkedin").on("click", function (e) {
        window.dataLayer = window.dataLayer || [];
        dataLayer.push({
            'event': 'socialInteraction',
            'socialNetwork': 'Linkedin',
            'socialAction': 'Share',
            'socialTarget': document.location.toString().split("?")[0] // strip off any url variables
        });
    });

    $(".ss-google, .s-google").on("click", function (e) {
        window.dataLayer = window.dataLayer || [];
        dataLayer.push({
            'event': 'socialInteraction',
            'socialNetwork': 'Google+',
            'socialAction': 'Share',
            'socialTarget': document.location.toString().split("?")[0] // strip off any url variables
        });
    });

    // New Go button
    $("#n5b-login-go").on("click", function (e) {
        window.dataLayer = window.dataLayer || [];
        dataLayer.push({
            "Value": $(".n5b-selected a").text(),
            "event": "OnlineServices"
        });
        CreateClientCookie();
    });

    // Private Bank online services menu
    $("#login-go").on("click", function (e) {
        e.preventDefault();
        window.dataLayer = window.dataLayer || [];
        dataLayer.push({
            "Value": $("#go_to option:selected").text(),
            "event": "OnlineServices"
        });

        var val = $("#go_to option:selected").val();
        var url = val.split("|");
        if (url[1] === "_self")
            document.location = url[0];
        else
            window.open(url[0]);

        CreateClientCookie();
    });

    // Private Bank online services menu
    $("#go_to").on("change", function (e) {
        e.preventDefault();
        window.dataLayer = window.dataLayer || [];
        dataLayer.push({
            "Value": $("#go_to option:selected").text(),
            "event": "OnlineServices"
        });

        var val = $("#go_to option:selected").val();
        var url = val.split("|");
        if (url[1] === "_self")
            document.location = url[0];
        else
            window.open(url[0]);

        CreateClientCookie();
    });

    $("#online-services-list a").on("click", function () {
        CreateClientCookie();
    });

    $.fn.OnAvailable = function (fn) {
        var sel = this.selector;
        var timer;
        var count = 0;
        var self = this;
        if (this.length > 0) {
            fn.call(this);
        } else {
            timer = setInterval(function () {
                count++;
                if (count > 15) {
                    clearInterval(timer);
                }
                if ($(sel).length > 0) {
                    fn.call($(sel));
                    clearInterval(timer);
                }
            }, 50);
        }
    };

    $("input#header-site-search").on("keydown", function (e) {
        if (e.which === 13 || e.keyCode === 13) {
            DoSiteSearch();
        }
    });

    var DoSiteSearch = function () {
        if (ValidateSiteSearch()) {
            var siteSearch = $("input#header-site-search");
            var term = siteSearch.val();

            $("form#header-search-form").trigger("submit");
        }
    };

    var ValidateSiteSearch = function () {
        var siteSearch = $("input#header-site-search");
        var term = siteSearch.val();

        if (term === "") {
            return false;
        }
        else {
            if (term.indexOf('<') !== -1 || term.indexOf('>') !== -1) {
                alert('<> symbols not allowed.');
                return false;
            }
            return true;
        }
    };
});

